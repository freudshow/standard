oojs.define({name:"page",namespace:"",arrowT:null,liT:null,autoT:null,lightNum:1,curItem:0,subLinkConWidth:0,$page:function(){try{var d=document.getElementById("subLinkCon");if(!d){return}this.subLinkConWidth=this.getAttr(d,"conWidth");this.adEvents();var a=this.getAttr(document.getElementById("container"),"estShowAnti");if(a!==undefined){var c=a.split(",");for(var b=0;b<c.length;b++){window.antiArray.push(parseInt(c[b],10))}}this.autoT=setTimeout(function(){document.getElementById("subLinkUl").style.display="none";document.getElementById("subLinkCon").style.width="16px";document.getElementById("subLinkArrow").style.backgroundImage="url(http://cpro.baidustatic.com/cpro/ui/noexpire/img/sublink/subItemRightArrow.png)"},5000)}catch(f){}},adEvents:function(){var b=document.getElementById("container");var d=document.getElementById("subLinkArrow");this.bind(d,"click",this.doArrowClick.proxy(this));this.bind(d,"mouseover",this.doArrowMouseOver.proxy(this));this.bind(d,"mouseout",this.doArrowMouseOut.proxy(this));var a=document.getElementById("subLinkUl");for(var c=0;c<a.children.length;c++){this.bind(a.children[c],"mouseover",this.doLiMouseOver.proxy(this));this.bind(a.children[c],"mouseout",this.doLiMouseOut.proxy(this))}},doArrowClick:function(a){clearInterval(this.autoT);var b=parseInt(document.getElementById("subLinkCon").clientWidth,10);if(b!==16){document.getElementById("subLinkUl").style.display="none";document.getElementById("subLinkCon").style.width="16px";document.getElementById("subLinkArrow").style.backgroundImage="url(http://cpro.baidustatic.com/cpro/ui/noexpire/img/sublink/subItemRightArrow.png)"}else{document.getElementById("subLinkUl").style.display="";document.getElementById("subLinkCon").style.width=this.subLinkConWidth;document.getElementById("subLinkArrow").style.backgroundImage="url(http://cpro.baidustatic.com/cpro/ui/noexpire/img/sublink/subItemLeftArrow.png)"}},doArrowMouseOver:function(a){clearTimeout(this.arrowT);clearInterval(this.autoT);clearTimeout(this.liT);var b=parseInt(document.getElementById("subLinkCon").clientWidth,10);if(b===16){document.getElementById("subLinkUl").style.display="";document.getElementById("subLinkCon").style.width=this.subLinkConWidth;document.getElementById("subLinkArrow").style.backgroundImage="url(http://cpro.baidustatic.com/cpro/ui/noexpire/img/sublink/subItemLeftArrow.png)"}},doArrowMouseOut:function(a){var b=parseInt(document.getElementById("subLinkCon").clientWidth,10);if(b!==16){this.arrowT=setTimeout(function(){document.getElementById("subLinkUl").style.display="none";document.getElementById("subLinkCon").style.width="16px";document.getElementById("subLinkArrow").style.backgroundImage="url(http://cpro.baidustatic.com/cpro/ui/noexpire/img/sublink/subItemRightArrow.png)"},1000)}},doLiMouseOver:function(a){clearInterval(this.autoT);clearTimeout(this.liT);clearTimeout(this.arrowT)},doLiMouseOut:function(a){this.liT=setTimeout(function(){document.getElementById("subLinkUl").style.display="none";document.getElementById("subLinkCon").style.width="16px";document.getElementById("subLinkArrow").style.backgroundImage="http://cpro.baidustatic.com/cpro/ui/noexpire/img/sublink/subItemRightArrow.png"},1000)},bind:function(a,c,b){if(a.addEventListener){a.addEventListener(c,b,false)}else{a.attachEvent("on"+c,b)}},getAttr:function(b,a){if(b&&b.getAttribute){return b.getAttribute(a)}else{return b[a]}},formatEventObj:function(a){a=a||window.event;a.target=a.target||a.srcElement;return a},stopEvent:function(a){if(a&&a.stopPropagation){a.stopPropagation()}else{window.event.cancelBubble=true}if(a&&a.preventDefault){a.preventDefault()}return false}});