oojs.define({name:"page",namespace:"",arrowT:null,liT:null,autoT:null,autoLight:true,lightNum:1,curItem:0,$page:function(){try{var d=document.getElementById("subLinkCon");if(!d){return}this.adEvents();var a=this.getAttr(document.getElementById("container"),"estShowAnti");if(a!==undefined){var c=a.split(",");for(var b=0;b<c.length;b++){window.antiArray.push(parseInt(c[b],10))}}this.doAutoLight().proxy(this)}catch(f){}},adEvents:function(){var c=document.getElementById("subLinkArrow");this.bind(c,"click",this.doArrowClick.proxy(this));this.bind(c,"mouseover",this.doArrowMouseOver.proxy(this));this.bind(c,"mouseout",this.doArrowMouseOut.proxy(this));var a=document.getElementById("subLinkUl");for(var b=0;b<a.children.length;b++){this.bind(a.children[b],"mouseover",this.doLiMouseOver.proxy(this));this.bind(a.children[b],"mouseout",this.doLiMouseOut.proxy(this))}},doArrowClick:function(a){clearInterval(this.autoT);var b=parseInt(document.getElementById("subLinkCon").clientWidth,10);if(b===110){document.getElementById("subLinkUl").style.display="none";document.getElementById("subLinkCon").style.width="16px"}else{document.getElementById("subLinkUl").style.display="";document.getElementById("subLinkCon").style.width="110px"}},doArrowMouseOver:function(a){clearTimeout(this.arrowT);clearInterval(this.autoT);var b=parseInt(document.getElementById("subLinkCon").clientWidth,10);if(b===16){document.getElementById("subLinkUl").style.display="";document.getElementById("subLinkCon").style.width="110px";this.autoLight=true}},doArrowMouseOut:function(a){var b=parseInt(document.getElementById("subLinkCon").clientWidth,10);if(b===110){this.arrowT=setTimeout(function(){document.getElementById("subLinkUl").style.display="none";document.getElementById("subLinkCon").style.width="16px"},5000)}},doLiMouseOver:function(a){this.autoLight=false;clearInterval(this.autoT);clearTimeout(this.liT)},doLiMouseOut:function(a){if(this.curItem!==0){document.getElementById("subLinkUl").children[this.curItem-1].style.backgroundColor="#f8f8f8"}this.liT=setTimeout(function(){document.getElementById("subLinkUl").style.display="none";document.getElementById("subLinkCon").style.width="16px"},5000)},doAutoLight:function(){document.getElementById("subLinkUl").children.item(0).style.backgroundColor="#ffc8ce";this.curItem++;this.autoT=setInterval((function(){var a=document.getElementById("subLinkUl").children;if(this.curItem===a.length||this.lightNum>a.length){a[this.curItem-1].style.backgroundColor="#f8f8f8";if(this.lightNum*2000<8000){this.curItem=0}else{clearTimeout(this.autoT);document.getElementById("subLinkUl").style.display="none";document.getElementById("subLinkCon").style.width="16px";return}}if(this.curItem-1>-1){a[this.curItem-1].style.backgroundColor="#f8f8f8"}a[this.curItem].style.backgroundColor="#ffc8ce";this.curItem++;this.lightNum++}).proxy(this),2000)},bind:function(a,c,b){if(a.addEventListener){a.addEventListener(c,b,false)}else{a.attachEvent("on"+c,b)}},getAttr:function(b,a){if(b&&b.getAttribute){return b.getAttribute(a)}else{return b[a]}},formatEventObj:function(a){a=a||window.event;a.target=a.target||a.srcElement;return a},stopEvent:function(a){if(a&&a.stopPropagation){a.stopPropagation()}else{window.event.cancelBubble=true}if(a&&a.preventDefault){a.preventDefault()}return false}});